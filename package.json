{
  "name": "personal-number",
  "version": "1.0.0",
  "title": "Czech personal identity number util",
  "description": "Validates and extracts information from Czech personal identity number",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest --config jest.config.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mild-blue/personal-number.git"
  },
  "keywords": [
    "czech",
    "personal number",
    "rodné číslo",
    "validation"
  ],
  "author": "Mild Blue s.r.o. <info@mild.blue>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mild-blue/personal-number/issues"
  },
  "homepage": "https://github.com/mild-blue/personal-number#readme",
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.4",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.3.5"
  }
}
